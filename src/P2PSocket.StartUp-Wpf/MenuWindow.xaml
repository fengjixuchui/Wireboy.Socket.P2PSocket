<Window x:Class="P2PSocket.StartUp_Wpf.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P2PSocket.StartUp_Wpf"
        mc:Ignorable="d"
        Title="P2PSocket" Height="250" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="直接启动" x:Name="btn_show" HorizontalAlignment="Left" Margin="105,56,0,0" VerticalAlignment="Top" Width="75" Click="btn_show_Click"/>
        <Button Content="安装服务" x:Name="btn_install" HorizontalAlignment="Left" Margin="105,96,0,0" VerticalAlignment="Top" Width="75" Click="btn_install_Click"/>
        <Button Content="启动服务" x:Name="btn_start" HorizontalAlignment="Left" Margin="105,141,0,0" VerticalAlignment="Top" Width="75" Click="btn_start_Click"/>

    </Grid>
</Window>
